import numpy as np
import matplotlib.pyplot as plt
overs = np.arange(1, 11)
scores = [1, 6, 20, 3, 5, 6, 13, 5, 7, 21]
plt.plot(overs, scores, marker='o', linestyle='--', color='blue')
plt.title("India's Score Progression Over 10 Overs")
plt.xlabel("Over Number")
plt.ylabel("Indian Scores")
plt.xticks(overs)
plt.show()
